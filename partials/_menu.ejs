<ul class="main-navigation">
  <% menu.forEach(function (item) { %>
  <li>
    <a class="btn btn-dark <% if (item.active) { %>active<% } %>" href="<%= base %><%= item.link %>"><%= item.title %></a>
  </li>
  <% }); %>
</ul>

<fieldset class="mobile-nav">
  <select>
    <option value="">Menu…</option>
    <% menu.forEach(function (item) { %>
    <option value="<%= base %><%= item.link %>">» <%= item.title %></option>
    <% }); %>
  </select>
</fieldset>
